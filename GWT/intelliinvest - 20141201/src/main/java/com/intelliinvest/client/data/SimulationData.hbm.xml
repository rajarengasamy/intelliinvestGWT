<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.intelliinvest.client.data">

    <class name="SimulationData">
    	<id name="code" column="code">
    		<generator class="assigned"/>
    	</id>
    	<property name="quarterly" column="quarterly"/>
    	<property name="halfYearly" column="halfYearly"/>
    	<property name="nineMonths" column="nineMonths"/>
    	<property name="yearly" column="yearly"/>
    </class>
    
</hibernate-mapping>