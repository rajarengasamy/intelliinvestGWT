<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.intelliinvest.client.data">

    <class name="TradingAccountData">
    	<id name="code" column="code">
    		<generator class="assigned"/>
    	</id>
    	<property name="yesterdaySignalType" column="yesterdaySignalType"/>
    	<property name="signalType" column="signalType"/>
    	<property name="signalPrice" column="signalPrice"/>
    	<property name="signalDate" column="signalDate"/>
    </class>
    
</hibernate-mapping>