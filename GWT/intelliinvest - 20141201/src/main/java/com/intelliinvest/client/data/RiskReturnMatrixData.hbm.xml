<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.intelliinvest.client.data">

    <class name="RiskReturnMatrixData">
   		<id name="code" column="code">
    		<generator class="assigned"/>
    	</id>
    	<property name="signalType" column="signalType"/>
   		<property name="type3m" column="type3m"/>
   		<property name="type6m" column="type6m"/>
   		<property name="type9m" column="type9m"/>
   		<property name="type12m" column="type12m"/>
    </class>
    
</hibernate-mapping>